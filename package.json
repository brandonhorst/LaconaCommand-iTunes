{
  "name": "lacona-itunes",
  "version": "0.0.3",
  "description": "Lacona Command to play media in iTunes",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p build; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-itunes.git"
  },
  "keywords": [
    "lacona",
    "command",
    "itunes",
    "play",
    "music",
    "video",
    "movie",
    "media"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-itunes/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-itunes",
  "dependencies": {
    "elliptical": "^1.0.0-rc10",
    "lacona-api": "0.1.2",
    "lacona-command": "^0.2.2",
    "lodash": "^4.9.0",
    "rxjs": "^5.0.0-beta.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
