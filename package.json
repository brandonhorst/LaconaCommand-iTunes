{
  "name": "lacona-itunes",
  "version": "0.1.1",
  "description": "Lacona Command to play media in iTunes",
  "lacona": {
    "extensions": "build/extensions.js"
  },
  "scripts": {
    "build:build": "mkdir -p build; browserify src/extensions.jsx -t babelify -o build/extensions.js -x lacona-phrases -x elliptical -x lacona-api --standalone extensions --extension=jsx",
    "build:lib": "babel src --out-dir lib",
    "clean": "rimraf build lib",
    "prepublish": "npm run clean && npm run build:build && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-itunes.git"
  },
  "keywords": [
    "lacona",
    "command",
    "itunes",
    "play",
    "music",
    "video",
    "movie",
    "media"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-itunes/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-itunes",
  "dependencies": {
    "lodash": "^4.13.1",
    "rxjs": "^5.0.0-beta.8"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
