{
  "name": "lacona-itunes",
  "version": "1.1.0",
  "description": "Lacona Command to play media in iTunes",
  "lacona": {
    "title": "iTunes",
    "customPreferences": true,
    "description": "Play music in iTunes."
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf build lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-itunes.git"
  },
  "keywords": [
    "lacona",
    "command",
    "itunes",
    "play",
    "music",
    "video",
    "movie",
    "media"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-itunes/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-itunes",
  "dependencies": {
    "elliptical": "^2.0.0",
    "lacona-api": "^1.4.1",
    "lacona-phrases": "^1.1.0",
    "lodash": "^4.16.4",
    "rxjs": "^5.0.0-rc.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  }
}
